<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_30"
    android:gravity="center_vertical">

    <ImageView
        android:id="@+id/id_treenode_icon"
        android:layout_width="@dimen/qb_px_14"
        android:layout_height="@dimen/qb_px_14"
        android:layout_marginLeft="@dimen/qb_px_4"
        android:layout_marginRight="@dimen/qb_px_4"
        android:src="@drawable/tree_ec" />

    <TextView
        android:id="@+id/id_treenode_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/app_name"
        android:textSize="14sp" />

    <ImageView
        android:id="@+id/id_select_icon"
        android:layout_width="@dimen/qb_px_16"
        android:layout_height="@dimen/qb_px_16"
        android:layout_marginLeft="@dimen/qb_px_4"
        android:layout_marginRight="@dimen/qb_px_4"
        android:src="@drawable/ic_arrow_left" />
</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bsd_m_top"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_36"
    android:background="#FFFFFF">

    <LinearLayout
        android:id="@+id/bsd_lsbj_fanhui"
        android:layout_width="@dimen/qb_px_80"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/qb_px_10"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="@dimen/qb_px_20"
            android:layout_height="@dimen/qb_px_16"
            android:scaleType="fitXY"
            android:layout_centerVertical="true"
            android:src="@drawable/ic_back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="返回"
            android:textColor="#636364"
            android:textSize="18sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="title"
        android:textColor="#636364"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_billNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/qb_px_10"
        android:layout_centerVertical="true"
        android:text="billno"
        android:textSize="18sp"
        android:textColor="#636364"/>
</RelativeLayout>
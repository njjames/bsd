<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_28"
    android:minHeight="@dimen/qb_px_28"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/bsd_xzcl_no"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="材料编码" />

    <TextView
        android:id="@+id/bsd_xzcl_name"
        android:layout_weight="1.2"
        style="@style/mysearchinfostyle"
        android:text="材料名称" />

    <TextView
        android:id="@+id/bsd_xzcl_th"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="图号" />

    <TextView
        android:id="@+id/bsd_xzcl_pp"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="品牌" />

    <TextView
        android:id="@+id/bsd_xzcl_shuliang"
        android:layout_weight="0.8"
        style="@style/mysearchinfostyle"
        android:text="数量" />

    <TextView
        android:id="@+id/bsd_xzcl_dw"
        android:layout_weight="0.5"
        style="@style/mysearchinfostyle"
        android:text="单位" />

    <TextView
        android:id="@+id/bsd_xzcl_danjia"
        android:layout_weight="0.8"
        style="@style/mysearchinfostyle"
        android:text="单价" />

    <TextView
        android:id="@+id/bsd_xzcl_je"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="金额" />
</LinearLayout>


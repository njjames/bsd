<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1F1F1">

    <!-- 头部 -->
    <RelativeLayout
        android:id="@+id/relativeLayout23"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y60"
        android:background="#FFFFFF">
        <RelativeLayout
            android:id="@+id/relativeLayout22"
            android:layout_width="@dimen/x100"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/x5">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="@dimen/x5"
                android:layout_height="@dimen/y25"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:src="@mipmap/bsd_fanhui" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/x5"
                android:layout_toEndOf="@+id/imageView5"
                android:text="返回"
                android:textColor="#636364"
                android:textSize="18sp" />

        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="输入车牌"
            android:textColor="#636364"
            android:textSize="20sp" />
    </RelativeLayout>
    <!-- 头部 结束-->
    <!--中间开始-->
    <RelativeLayout
        android:id="@+id/bsd_lsbj_zhong"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/x5"
        android:layout_marginRight="@dimen/x5"
        android:layout_marginTop="@dimen/y10"
        android:layout_marginBottom="@dimen/y10"
        android:paddingLeft="@dimen/x10"
        android:paddingRight="@dimen/x10"
        android:background="@drawable/bsd_lsbj_body"
        android:layout_below="@+id/relativeLayout23"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:id="@+id/ll_car_license_inputbox_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y150"
            android:background="@android:color/white"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="@dimen/y20"
                android:text="请输入车牌号"
                android:textColor="#9DA9B4"
                android:textSize="26sp" />

            <LinearLayout
                android:id="@+id/ll_license_input_boxes_content"
                android:layout_width="@dimen/x260"
                android:layout_height="@dimen/y130"
                android:background="@drawable/cepai"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/et_car_license_inputbox1"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical"
                    android:maxEms="1"/>

                <EditText
                    android:id="@+id/et_car_license_inputbox2"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical"
                    android:maxEms="1"/>

                <EditText
                    android:id="@+id/et_car_license_inputbox3"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical"
                    android:maxEms="1"/>

                <EditText
                    android:id="@+id/et_car_license_inputbox4"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical"
                    android:maxEms="1"/>

                <EditText
                    android:id="@+id/et_car_license_inputbox5"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical"
                    android:maxEms="1"/>

                <EditText
                    android:id="@+id/et_car_license_inputbox6"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical" />

                <EditText
                    android:id="@+id/et_car_license_inputbox7"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical" />

                <EditText
                    android:id="@+id/et_car_license_inputbox8"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical" />

                <EditText
                    android:id="@+id/et_car_license_inputbox9"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical" />
                <EditText
                    android:id="@+id/et_car_license_inputbox10"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@null"
                    android:gravity="center_horizontal|center_vertical" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="vertical">
            <android.inputmethodservice.KeyboardView
                android:id="@+id/bsd_keyboard_view1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:focusable="true"
                android:paddingTop="5dp"
                android:layout_gravity="center_horizontal"
                android:background="#F0EFF4"
                android:keyBackground="@android:color/white"
                android:keyTextColor="#5E5E5E"
                android:shadowRadius="0.0"
                android:keyTextSize="18sp"
                android:labelTextSize="18sp"
                android:keyPreviewLayout="@layout/key_ios"
                android:visibility="gone" />
        </LinearLayout>


    </RelativeLayout>
    <!--中间结束-->


</RelativeLayout>

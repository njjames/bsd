<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bsd_rl_xuanxiang"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_30"
    android:minHeight="@dimen/qb_px_30"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/bsd_xsbj_no"
        android:layout_weight="1.2"
        style="@style/mysearchinfostyle"
        android:text="单据号" />

    <TextView
        android:id="@+id/bsd_kxbj_kehumc"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="客户名称" />

    <TextView
        android:id="@+id/bsd_kxbj_cheno"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="车牌号" />

    <TextView
        android:id="@+id/bsd_kxbj_jdrq"
        android:layout_weight="1.2"
        style="@style/mysearchinfostyle"
        android:text="接待日期" />

    <TextView
        android:id="@+id/bsd_kxbj_fwgw"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="服务顾问" />

    <TextView
        android:id="@+id/bsd_kxbj_phone"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="联系方式" />

    <TextView
        android:id="@+id/bsd_kxbj_je"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="合计金额" />
</LinearLayout>
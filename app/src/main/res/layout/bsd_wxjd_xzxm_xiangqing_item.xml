<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_30"
    android:minHeight="@dimen/qb_px_30"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/bsd_xzcl_name"
        style="@style/mysearchinfostyle"
        android:layout_weight="1.2"
        android:text="项目名称" />

    <TextView
        android:id="@+id/bsd_xzcl_gs"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="工时" />

    <TextView
        android:id="@+id/bsd_xzcl_dj"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="单价" />

    <TextView
        android:id="@+id/bsd_xzcl_je"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="金额" />

    <TextView
        android:id="@+id/bsd_xzcl_zt"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="状态" />
</LinearLayout>

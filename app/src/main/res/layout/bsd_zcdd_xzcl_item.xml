<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bsd_rl_xuanxiang"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_28"
    android:minHeight="@dimen/qb_px_28"
    android:orientation="horizontal">

        <TextView
            android:id="@+id/bsd_xzcl_name"
            android:layout_weight="1"
            style="@style/mysearchinfostyle"
            android:text="材料名称" />


        <TextView
            android:id="@+id/bsd_xzcl_shuliang"
            android:layout_weight="1"
            style="@style/mysearchinfostyle"
            android:text="数量" />

        <TextView
            android:id="@+id/bsd_xzcl_danjia"
            android:layout_weight="1"
            style="@style/mysearchinfostyle"
            android:text="单价" />


        <TextView
            android:id="@+id/bsd_xzcl_dw"
            android:layout_weight="1"
            style="@style/mysearchinfostyle"
            android:text="图号" />


        <TextView
            android:id="@+id/bsd_xzcl_je"
            android:layout_weight="1"
            style="@style/mysearchinfostyle"
            android:text="品牌" />

        <TextView
            android:id="@+id/bsd_xzcl_caozuo"
            android:layout_weight="1"
            style="@style/mysearchinfostyle"
            android:text="金额" />
</LinearLayout>

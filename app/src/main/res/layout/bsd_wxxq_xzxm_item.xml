<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_28"
    android:minHeight="@dimen/qb_px_28"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/bsd_xzcl_name"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="项目名称" />

    <TextView
        android:id="@+id/bsd_xzcl_shuliang"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="标准工时" />


    <TextView
        android:id="@+id/bsd_xzcl_danjia"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="工时单价" />


    <TextView
        android:id="@+id/bsd_xzcl_yuangongshifei"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="原工时费" />

    <TextView
        android:id="@+id/bsd_xzcl_huiyuanzhekou"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="会员折扣" />

    <TextView
        android:id="@+id/bsd_xzcl_dw"
        style="@style/mysearchinfostyle"
        android:layout_weight="1"
        android:text="金额" />
</LinearLayout>

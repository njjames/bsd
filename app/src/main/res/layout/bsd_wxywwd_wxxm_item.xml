<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/bsd_xm1"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_below="@+id/view2"
        android:background="@drawable/biankuang_info_bg">

        <TextView
            android:id="@+id/bsd_xsbj_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="客户名称" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bsd_xm2"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_below="@+id/view2"
        android:layout_toRightOf="@+id/bsd_xm1"
        android:background="@drawable/biankuang_info_bg">

        <TextView
            android:id="@+id/bsd_kxbj_bzsj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="车主" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bsd_xm3"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_below="@+id/view2"
        android:layout_toRightOf="@+id/bsd_xm2"
        android:background="@drawable/biankuang_info_bg">

        <TextView
            android:id="@+id/bsd_kxbj_gsdj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="日期" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bsd_xm4"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_below="@+id/view2"
        android:layout_toRightOf="@+id/bsd_xm3"
        android:background="@drawable/biankuang_info_bg">

        <TextView
            android:id="@+id/bsd_kxbj_je"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="车牌" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bsd_xm5"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_below="@+id/view2"
        android:layout_toRightOf="@+id/bsd_xm4"
        android:background="@drawable/biankuang_info_bg">

        <TextView
            android:id="@+id/bsd_kxbj_je"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="车型" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bsd_xm6"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_below="@+id/view2"
        android:layout_toRightOf="@+id/bsd_xm5"
        android:background="@drawable/biankuang_info_bg">

        <TextView
            android:id="@+id/bsd_kxbj_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="服务" />




    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bsd_xm7"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/biankuang_info_bg"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/bsd_xm6">

        <TextView
            android:id="@+id/bsd_kxbj_qian1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="金额" />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/bsd_xm8"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:background="@drawable/biankuang_info_bg"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/bsd_xm7">

        <TextView
            android:id="@+id/bsd_kxbj_qian2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="查看详情" />

    </RelativeLayout>
</RelativeLayout>
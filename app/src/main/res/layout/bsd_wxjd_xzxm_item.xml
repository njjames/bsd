<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bsd_rl_xuanxiang"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_30"
    android:minHeight="@dimen/qb_px_30"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/bsd_xzcl_no"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="项目编码" />

    <TextView
        android:id="@+id/bsd_xzcl_name"
        android:layout_weight="1.2"
        style="@style/mysearchinfostyle"
        android:text="项目名称" />

    <TextView
        android:id="@+id/bsd_xzcl_gs"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="工时" />

    <TextView
        android:id="@+id/bsd_xzcl_danjia"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="单价" />

    <TextView
        android:id="@+id/bsd_xzcl_je"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="金额" />

    <TextView
        android:id="@+id/bsd_xzcl_zt"
        android:layout_weight="1"
        style="@style/mysearchinfostyle"
        android:text="状态" />

    <ImageView
        android:id="@+id/iv_delete"
        android:layout_weight="0.6"
        android:layout_width="0dp"
        android:layout_height="@dimen/qb_px_30"
        android:src="@drawable/ic_delete_maincolor"
        android:padding="@dimen/qb_px_8"
        android:background="@drawable/biankuang_info_bg" />
</LinearLayout>

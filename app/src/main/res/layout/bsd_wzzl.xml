<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bsd_log_wangzhezhilu"
    android:orientation="vertical"
    android:padding="@dimen/qb_px_18">

    <LinearLayout
        android:id="@+id/bsd_wzll_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qb_px_10"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/bsd_wzzl_ip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="IP地址："
            android:textColor="#000000"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/bsd_wzzl_et_ip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bsd_wzll_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qb_px_10"
        android:gravity="center_vertical">

        <CheckBox
            android:id="@+id/checkBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/qb_px_5"
            android:text="启用王者之路"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bsd_wzll_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qb_px_10">


        <TextView
            android:id="@+id/bsd_wzll_zh1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="账号："
            android:textColor="#000000"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/bsd_wzzl_et_user"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bsd_wzll_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qb_px_10">

        <TextView
            android:id="@+id/bsd_wzll_zh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="密码："
            android:textColor="#000000"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/bsd_wzzl_et_password"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bsd_wzll_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qb_px_10">

        <TextView
            android:id="@+id/bsd_wzll_zh3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="端口："
            android:textColor="#000000"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/bsd_wzzl_et_dk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="@dimen/qb_px_10">

        <TextView
            android:id="@+id/bsd_wzzl_tv_qx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bsd_log_wzzzl_queren_quxiao"
            android:gravity="center"
            android:text="取消"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:paddingTop="@dimen/qb_px_16"
            android:paddingBottom="@dimen/qb_px_16"
            android:paddingLeft="@dimen/qb_px_60"
            android:paddingRight="@dimen/qb_px_60"
            android:layout_marginLeft="@dimen/qb_px_40"
            android:layout_marginRight="@dimen/qb_px_10"/>

        <TextView
            android:id="@+id/bsd_wzzl_tijiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/qb_px_10"
            android:layout_marginRight="@dimen/qb_px_40"
            android:background="@drawable/bsd_log_wzzzl_queren_quxiao"
            android:gravity="center"
            android:text="提交"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            android:paddingTop="@dimen/qb_px_16"
            android:paddingBottom="@dimen/qb_px_16"
            android:paddingLeft="@dimen/qb_px_60"
            android:paddingRight="@dimen/qb_px_60"/>
    </LinearLayout>
</LinearLayout>
